// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"      // <--- Assurez-vous que c'est bien "sqlite"
  url      = env("DATABASE_URL")
}

// Ajoutez ou vérifiez que le modèle Product est présent :
model Product {
  id          String   @id @default(cuid())
  name        String
  description String
  price       Int      
  image       String
  category    String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}